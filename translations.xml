			<source valid="" exif="TrackLyricsMimeType"/>
			<source valid="" exif="Transformation"/>
			<source valid="" exif="VideoFormat"/>
		</value>
		<value ns="lom" tag="duration">
			<!-- unordered -->
			<source valid="" exif="DefaultDuration"/>
				<source valid="" exif="Duration"/>
				<source valid="" exif="Editing-duration"/>
				<source valid="" exif="MediaDuration"/>
				<source valid="" exif="PlayDuration"/>
				<source valid="" exif="PreviewDuration"/>
				<source valid="" exif="StreamDuration"/>
				<source valid="" exif="TrackDuration"/>
		</value>
	</group>
	<group lom="rights">
		<value ns="lom" tag="copydescription">
			<source ordered="true" valid="true" ns="http://ns.exiftool.ca/XMP/XMP-dc/1.0/" exif="Rights"/>
			<source ordered="true" valid="true" exif="Rights-fr"/>
			<source ordered="true" valid="true" exif="Rights"/>
			<source ordered="true" valid="false" ns="http://ns.exiftool.ca/PSP/PSP/1.0/" exif="Copyright"/>
			<source ordered="true" valid="false" ns="http://ns.exiftool.ca/EXIF/IFD0/1.0/" exif="Copyright"/>
			<source ordered="true" valid="false" ns="http://ns.exiftool.ca/Real/Real-RA4/1.0/" exif="Copyright"/>
			<source ordered="true" valid="true" exif="Copyright"/>
			<source ordered="true" valid="true" exif="Copyright-fr"/>
			<source ordered="true" valid="true" exif="Copyright-fr-CA"/>
			<source ordered="true" valid="true" exif="Copyright-fr-FR"/>
			<source ordered="true" valid="true" exif="Copyright-en"/>
			<source ordered="true" valid="true" exif="Copyright-en-US"/>
			<source ordered="true" valid="true" exif="Copyright-he"/>
			<source ordered="true" valid="true" exif="CopyrightNotice"/>
			<!-- unordered -->
			<source valid="true" exif="LegalCopyright"/>
		</value>
	</group>
</transformation>
