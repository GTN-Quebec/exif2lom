			<source valid="" exif="RawImageSize"/>
			<source valid="" exif="RawJpgSize"/>
			<source valid="" exif="ResourceForkSize"/>
			<source valid="" exif="TargetFileSize"/>
			<source valid="" exif="VideoSize"/>
		</value>
		<value ns="lom" tag="format">
			<!-- unordered -->
			<source valid="" exif="AudioFormat"/>
			<source valid="" exif="CoverArtMIMEType"/>
			<source valid="" exif="FileFormat"/>
			<source valid="" exif="FocalLengthIn35mmFormat"/>
			<source valid="" exif="Format"/>
			<source valid="" exif="ImageFormat"/>
			<source valid="" exif="Information"/>
			<source valid="" exif="MIMEType"/>
